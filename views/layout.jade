						span(class="icon-bar")
						span(class="icon-bar")
					a(class="brand", href="/") tryKendo
					div(class="nav-collapse")
						ul(class="nav")
							li 
								a(href="/") Home
							li
								a(href="/dojo") Dojo
							li
								a(href="/about") About Kendo UI
							li
								a(href="/contact") Contact Us
