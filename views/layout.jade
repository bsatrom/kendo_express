!!!
html
	head
		meta(charset="utf-8")
		title= title
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		meta(name="description", content="tryKendo, an online learning resource for Kendo UI")
		meta(name="author", content="Brandon Satrom")
		link(rel='stylesheet', href='/stylesheets/lib/bootstrap.min.css')
		style(type="text/css")
			body {
				padding-top: 60px;
				padding-bottom: 40px;
			}
		link(rel='stylesheet', href='/stylesheets/lib/bootstrap-responsive.min.css')
		block stylesheets
		block scripts
			script(src="../javascripts/require-jquery.js", data-main="javascripts/main")

		// HTML5 shim, for IE6-8 support of HTML5 elements
